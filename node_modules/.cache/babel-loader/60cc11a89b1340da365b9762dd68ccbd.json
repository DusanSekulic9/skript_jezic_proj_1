telefon = JSON.stringify({marka: this.newMarka, model: this.newModel, procesor: this.newProcesor, verzija: this.newVerzija});\n                let { error } = Joi.validate(telefon, sema);\n\n                if(error){\n                    alert(error.details[0].message + 123);\n                  return;\n                }\n\n                if (!this.$route.params.id)\n                    this.new_telefon(telefon);\n                else\n                    this.change_telefon({id: this.$route.params.id, tel: telefon});\n\n                this.newMarka = '';\n                this.newModel = '';\n                this.newProcesor = '';\n                this.newVerzija = '';\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}